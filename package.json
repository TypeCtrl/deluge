{
  "name": "@ctrl/deluge",
  "version": "0.0.0-placeholder",
  "description": "TypeScript api wrapper for deluge using got",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "typectrl/deluge",
  "keywords": [
    "deluge",
    "typescript"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:ci": "jest --runInBand --coverage --no-cache",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@ctrl/shared-torrent": "^2.0.0",
    "cookiejar": "^2.1.2",
    "cross-fetch": "^3.0.4",
    "form-data": "^3.0.0",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@types/cookiejar": "^2.1.1",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.8",
    "@types/tough-cookie": "4.0.0",
    "@types/url-join": "4.0.0",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "eslint": "6.8.0",
    "eslint-config-xo-space": "0.24.0",
    "eslint-config-xo-typescript": "0.27.0",
    "eslint-plugin-import": "2.20.2",
    "jest": "25.2.4",
    "p-wait-for": "3.1.0",
    "semantic-release": "17.0.4",
    "ts-jest": "25.3.0",
    "typedoc": "0.17.3",
    "typescript": "3.8.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">= 8"
  }
}
