{
  "name": "@ctrl/deluge",
  "version": "0.0.0-placeholder",
  "description": "TypeScript api wrapper for deluge using got",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "typectrl/deluge",
  "keywords": [
    "deluge",
    "typescript"
  ],
  "source": "src/index.ts",
  "main": "./deluge.js",
  "module": "./deluge.m.js",
  "typings": "./src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "tslint -t stylish -p .",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "microbundle build --target node --format cjs,es -o dist",
    "postbuild": "cp package.json dist && cp LICENSE dist && cp README.md dist && rimraf dist/test",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:ci": "jest --runInBand --coverage --no-cache",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {
    "form-data": "2.3.3",
    "got": "9.3.2",
    "tough-cookie": "2.5.0"
  },
  "devDependencies": {
    "@types/form-data": "2.2.1",
    "@types/got": "9.2.1",
    "@types/jest": "23.3.11",
    "@types/node": "10.12.18",
    "@types/p-wait-for": "2.0.0",
    "@types/tough-cookie": "2.3.4",
    "jest": "23.6.0",
    "microbundle": "0.7.0",
    "p-wait-for": "2.0.1",
    "prettier": "1.15.3",
    "rimraf": "2.6.2",
    "semantic-release": "15.13.2",
    "ts-jest": "23.10.5",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.17.0",
    "tslint-xo": "0.10.0",
    "typedoc": "0.13.0",
    "typescript": "3.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8.9.0"
  }
}
